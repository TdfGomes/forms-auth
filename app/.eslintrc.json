{
    "extends": "react-app",
    "rules":{
        "semi": [
            "error",
            "never"
        ],
        "quotes": [ "error", "prefer-double"],
        "indent": [ "warn",2],
        "indent-props": [ "warn",2],
        "pascal-case": "error",
        "tag-spacing": [ "error",{"beforeSelfClosing": "always"}]
    }
}